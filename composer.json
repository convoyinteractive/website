{
    "name": "convoyinteractive/convoy-next",
    "description": "The Convoy Interactive Website",
    "type": "project",
    "require": {
        "php": "^8.0",
        "drewm/mailchimp-api": "^2.5",
        "erusev/parsedown": "^1.7",
        "erusev/parsedown-extra": "^0.8.1",
        "htmlmin/htmlmin": "^8.0",
        "laravel/lumen-framework": "^8.0",
        "symfony/yaml": "^5.0"
    },
    "autoload": {
        "files": [
            "helpers.php"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
